{
  "name": "@fastybird/ws-exchange-plugin",
  "version": "1.0.0",
  "description": "FastyBird IoT WS WAMP client & server plugin for handling UI communication",
  "keywords": [
    "vue",
    "wamp",
    "wampv1",
    "websockets",
    "ws"
  ],
  "homepage": "https://www.fastybird.com",
  "bugs": "https://github.com/FastyBird/ws-exchange-plugin/issues",
  "license": "Apache-2.0",
  "author": {
    "name": "FastyBird s.r.o.",
    "email": "info@fastybird.com",
    "url": "https://www.fastybird.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FastyBird/ws-exchange-plugin.git"
  },
  "main": "dist/ws-exchange-plugin.ssr.js",
  "browser": "dist/ws-exchange-plugin.esm.js",
  "module": "dist/ws-exchange-plugin.esm.js",
  "unpkg": "dist/ws-exchange-plugin.min.js",
  "types": "dist/ws-exchange-plugin.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "lint:js": "eslint public/**/* --ext \".js,.ts,.json,\" --ignore-path .gitignore public/",
    "lint:ts": "tslint -c tslint.json 'public/**/*.ts'"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9",
    "@babel/plugin-proposal-class-properties": "^7.10",
    "@babel/preset-env": "^7.9",
    "@babel/preset-typescript": "^7.9",
    "@rollup/plugin-alias": "^2.2",
    "@rollup/plugin-babel": "^5.3",
    "@rollup/plugin-commonjs": "^11.1",
    "@rollup/plugin-eslint": "8.0.2",
    "@rollup/plugin-replace": "^2.3",
    "@types/lodash": "^4.14",
    "@types/node": "^15.12",
    "@types/uuid": "^8.3",
    "@typescript-eslint/eslint-plugin": "^4.26",
    "@typescript-eslint/parser": "^4.26",
    "cross-env": "^7.0",
    "eslint": "^7.28",
    "eslint-plugin-json": "^3.0",
    "minimist": "^1.2",
    "rollup": "^2.7",
    "rollup-plugin-dts": "^3.0",
    "rollup-plugin-terser": "^5.3",
    "tslib": "^2.2",
    "tslint": "^6.1",
    "typescript": "^4.3",
    "vue": "^3.2"
  },
  "peerDependencies": {
    "vue": "^3.2"
  },
  "engines": {
    "node": ">=10"
  }
}
