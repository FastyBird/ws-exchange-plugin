{
  "name" : "fastybird/ws-exchange-plugin",
  "type" : "plugin",
  "description" : "FastyBird IoT WS WAMP client & server plugin for handling UI communication",
  "keywords" : [
    "fastybird",
    "fb",
    "libs",
    "library",
    "ws",
    "realtime"
  ],

  "homepage" : "https://www.fastybird.com",

  "license" : "Apache-2.0",

  "authors" : [
    {
      "name" : "FastyBird s.r.o.",
      "email" : "code@fastybird.com",
      "homepage" : "https://www.fastybird.com"
    },
    {
      "name" : "Adam Kadlec",
      "email" : "adam.kadlec@fastybird.com"
    }
  ],

  "support" : {
    "email" : "code@fastybird.com",
    "issues" : "https://github.com/FastyBird/ws-exchange-plugin/issues",
    "source" : "https://github.com/FastyBird/ws-exchange-plugin"
  },

  "prefer-stable" : true,

  "minimum-stability" : "dev",

  "require" : {
    "php" : ">=8.1.0",
    "cweagans/composer-patches" : "^1.7",
    "fastybird/exchange" : "^1.0",
    "fastybird/metadata" : "^1.1",
    "ipub/websockets-wamp" : "^1.4",
    "nette/di" : "^3.0",
    "nette/utils" : "^3.2",
    "psr/event-dispatcher" : "^1.0",
    "psr/log" : "^1.1|^3.0",
    "symfony/event-dispatcher" : "^5.4",
    "symfony/console" : "^5.0|^6.0"
  },

  "require-dev" : {
    "brianium/paratest": "^6.6",
    "dg/bypass-finals": "^1.4",
    "infection/infection": "^0.26",
    "orisai/coding-standard": "^3.2",
    "pds/skeleton": "^1.0",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^1.8",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.1",
    "phpstan/phpstan-phpunit": "^1.1",
    "phpstan/phpstan-strict-rules": "^1.4",
    "phpunit/phpunit": "^9.5",
    "staabm/annotate-pull-request-from-checkstyle": "^1.8",
    "tracy/tracy": "^2.9"
  },

  "autoload" : {
    "psr-4" : {
      "FastyBird\\WsExchangePlugin\\" : "src/"
    }
  },

  "autoload-dev" : {
    "psr-4" : {
      "FastyBird\\WsExchangePlugin\\Tests\\Cases\\Unit\\" : "tests/cases/unit"
    }
  },

  "config" : {
    "sort-packages" : true,
    "allow-plugins" : {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "cweagans/composer-patches": true,
      "phpstan/extension-installer": true,
      "infection/extension-installer": true
    }
  }
}
